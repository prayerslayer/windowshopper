html
  head
    title= "Windowshopper"
    link(href='https://fonts.googleapis.com/css?family=Open+Sans:400,700', rel='stylesheet' type='text/css')
    link(rel="stylesheet", href="/styles/index.css")
  body
    h1= "Window shopper"
    ul(class="uis")
      each ui in uis
        li(class="ui")
          h2= ui.id
          a(href=ui.url)
            img(src=ui.image_url)